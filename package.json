{
  "name": "dg-form-validator",
  "version": "1.0.2",
  "description": "Yet another form validator for React components",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corellian/dg-form-validator.git"
  },
  "keywords": [
    "react",
    "form",
    "validator"
  ],
  "author": "Cristian Peraferrer <corellian.c@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corellian/dg-form-validator/issues"
  },
  "homepage": "https://github.com/corellian/dg-form-validator#readme",
  "dependencies": {
    "moment": "^2.22.2",
    "immutability-helper": "^2.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "eslint": "^5.8.0",
    "eslint-config-es6": "^1.10.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 7"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true,
      "spread": true,
      "restParams": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
